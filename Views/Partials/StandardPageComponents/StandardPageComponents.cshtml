@inherits UmbracoViewPage

@{
    StandardPageComponents pageComponentsNode = Model.Children<StandardPageComponents>().FirstOrDefault();

    if (pageComponentsNode != null)
    {
        foreach (IPublishedContent component in pageComponentsNode.Children)
        {
            switch (component.DocumentTypeAlias)
            {
                case Sc_AccordionTab_AN.ModelTypeAlias:
                    @Html.Partial("StandardPageComponents/SC_AccordionTab", (Sc_AccordionTab_AN) component, new ViewDataDictionary { { "pageType", "standard" } })
                    break;
                case Sc_Resources_AN.ModelTypeAlias:
                    @Html.Partial("StandardPageComponents/SC_Resources", (Sc_Resources_AN) component, new ViewDataDictionary { { "pageType", "standard" } })
                    break;
                case Sc_Text_AN.ModelTypeAlias:
                    @Html.Partial("StandardPageComponents/SC_Text", (Sc_Text_AN) component, new ViewDataDictionary { { "pageType", "standard" } })
                    break;
                case Sc_Alert_AN.ModelTypeAlias:
                    @Html.Partial("StandardPageComponents/SC_Alert", (Sc_Alert_AN) component, new ViewDataDictionary { { "pageType", "standard" } })
                    break;
                case Sc_Links_AN.ModelTypeAlias:
                    @Html.Partial("StandardPageComponents/SC_Links", (Sc_Links_AN) component, new ViewDataDictionary { { "pageType", "standard" } })
                    break;
                case Sc_Gallery_AN.ModelTypeAlias:
                    @Html.Partial("StandardPageComponents/SC_Gallery", (Sc_Gallery_AN) component, new ViewDataDictionary { { "pageType", "standard" } })
                    break;
                case Sc_Quotes_AN.ModelTypeAlias:
                    @Html.Partial("StandardPageComponents/SC_Quotes", (Sc_Quotes_AN) component, new ViewDataDictionary { { "pageType", "standard" } })
                    break;
                case Sc_Code_AN.ModelTypeAlias:
                    @Html.Partial("StandardPageComponents/SC_Code", (Sc_Code_AN) component, new ViewDataDictionary { { "pageType", "standard" } })
                    break;
                case Sc_PriceList_AN.ModelTypeAlias:
                    @Html.Partial("StandardPageComponents/SC_PriceList", (Sc_PriceList_AN) component, new ViewDataDictionary { { "pageType", "standard" } })
                    break;
                case Sc_Form_AN.ModelTypeAlias:
                    @Html.Partial("StandardPageComponents/SC_Form", (Sc_Form_AN)component, new ViewDataDictionary { { "pageType", "standard" }, { "globalSettingsID", Model.GlobalSettings.Id } })
                    break;
                case Sc_GridLayout_AN.ModelTypeAlias:
                    @Html.Partial("StandardPageComponents/SC_GridLayout", (Sc_GridLayout_AN)component, new ViewDataDictionary { { "pageType", "standard" } })
                    break;
                case Sc_SubpageListing_AN.ModelTypeAlias:
                    @Html.Partial("StandardPageComponents/SC_SubpageListing", (Sc_SubpageListing_AN)component, new ViewDataDictionary { { "pageType", "standard" }, { "globalSettingsID", Model.GlobalSettings.Id } })
                    break;
                case Sc_Pods_AN.ModelTypeAlias:
                    @Html.Partial("StandardPageComponents/SC_Pods", (Sc_Pods_AN)component, new ViewDataDictionary { { "pageType", "standard" } })
                    break;
                case Sc_ReusablePods_AN.ModelTypeAlias:
                    @Html.Partial("StandardPageComponents/SC_ReusablePods", (Sc_ReusablePods_AN)component, new ViewDataDictionary { { "pageType", "standard" }, { "globalSettingsID", Model.GlobalSettings.Id } })
                    break;
                case Sc_RelatedContent_AN.ModelTypeAlias:
                    @Html.Partial("StandardPageComponents/SC_RelatedContent", (Sc_RelatedContent_AN)component, new ViewDataDictionary { { "pageType", "standard" }, { "globalSettingsID", Model.GlobalSettings.Id } })
                    break;
                case Sc_Sitemap_AN.ModelTypeAlias:
                    @Html.Partial("StandardPageComponents/SC_Sitemap", (Sc_Sitemap_AN)component, new ViewDataDictionary { { "pageType", "standard" } })
                    break;
            }
        }
    }
}