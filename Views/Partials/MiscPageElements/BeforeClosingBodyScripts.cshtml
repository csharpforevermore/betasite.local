@inherits UmbracoViewPage

@{
    var globalSettings = (Umbraco.Web.PublishedModels.GlobalSettings)Model.GlobalSettings();
    
	@Html.Raw(globalSettings.BeforeClosingBodyScripts)

    if (Model.HasValue("pageBeforeClosingBodyScripts"))
    {
        @Html.Raw(Model.Value<string>("pageBeforeClosingBodyScripts"))
    }
}